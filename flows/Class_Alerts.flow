    <formulas>
        <description>Number of Contacts in String</description>
        <name>Counter</name>
        <dataType>Number</dataType>
        <expression>CEILING({!Length}/20)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>get the Contact ID of the user record for who is running the flow</description>
        <name>form_UserContactID</name>
        <dataType>String</dataType>
