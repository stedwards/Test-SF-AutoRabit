    </formulas>
    <formulas>
        <description>Length of the Contact ID String</description>
        <name>Length</name>
        <dataType>Number</dataType>
        <expression>LEN({!var_SelectedContacts})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Stores the remaining Contacts from the string yet to be parsed</description>
        <name>RemainingContacts</name>
        <dataType>String</dataType>
        <expression>TRIM(RIGHT({!var_SelectedContacts},({!Length}-20) ))</expression>
    </formulas>
    <formulas>
        <description>Stores a single Contact ID after parsing</description>
        <name>Single_ContactID</name>
        <dataType>String</dataType>
        <expression>LEFT( {!var_SelectedContacts},18 )</expression>
    </formulas>
