    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Location_Group__c</fullName>
        <description>Used to help group for reporting only. Do not place on layout.</description>
        <externalId>false</externalId>
        <formula>case(1, 
if( contains(Location, &quot;person&quot;) || contains(Location, &quot;office&quot;),1,0),&quot;In Person&quot;,
if( contains(Location, &quot;Roaming&quot;) ,1,0),&quot;Roaming&quot;,
if( contains(Location, &quot;Walk&quot;) ,1,0),&quot;Walk-in&quot;,
if( contains(Location, &quot;phone&quot;) ,1,0),&quot;Phone&quot;,
&quot;Unknown&quot;)</formula>
        <label>Location Group</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>sfal__AdvisingApptTopic__c</fullName>
        <deprecated>false</deprecated>
        <description>If this activity is an advising appointment, then this represents the topic of the advising appointment, i.e. grade concern, application deadline, etc.</description>
        <externalId>false</externalId>
        <inlineHelpText>If this activity is an advising appointment, then this represents the topic of the advising appointment, i.e. grade concern, application deadline, etc.</inlineHelpText>
        <label>Advising Appointment Topic</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>sfal__AdvisingApptType__c</fullName>
        <deprecated>false</deprecated>
        <description>If this activity is an advising appointment, then this represents the type of advising appointment, i.e. financial, academic, health, etc.</description>
        <externalId>false</externalId>
        <inlineHelpText>If this activity is an advising appointment, then this represents the type of advising appointment, i.e. financial, academic, health, etc.</inlineHelpText>
        <label>Advising Appointment Type</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>sfal__IsAdvisingEvent__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
    ISBLANK($Setup.sfal__SAL_Settings__c.sfal__Advising_Event_RecordType__c),
    false,
    IF(
        CONTAINS(
            $Setup.sfal__SAL_Settings__c.sfal__Advising_Event_RecordType__c,
            &quot;.&quot;
        ),
        $Setup.sfal__SAL_Settings__c.sfal__Advising_Event_RecordType__c == RecordType.NamespacePrefix &amp; &quot;.&quot; &amp; RecordType.DeveloperName,
        ISBLANK(RecordType.NamespacePrefix) &amp;&amp; $Setup.sfal__SAL_Settings__c.sfal__Advising_Event_RecordType__c == RecordType.DeveloperName
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Advising Event</label>
        <type>Checkbox</type>
    </fields>
